package com.test1;

class Fu{
  String name;
  public Fu(){
	  System.out.println("我是父类的无参构造函数");
  }
	  
  public Fu(String name){
	  this.name=name;
  }
  public void eat(){
	  System.out.println(name+"吃。。");
	  }
}

class Zi extends Fu{
	String num;
	
	public Zi(){
		  System.out.println("我是子类的无参构造函数");	
	}
}	
	
 class Demo1 {
	public static void main(String[] args) {
		
		Zi z=new Zi();
		
	}
}


